    @{
    #endif
    }
    @* End: WASM *@

    @* Server: Block *@
    @{
    #if !(WASM)
    }
    <p>More information about the SfPdfViewer can be found in this <a target="_blank" href="https://help.syncfusion.com/document-processing/pdf/pdf-viewer/blazor/toolbar-customization" aria-label="Navigate to the documentation for toolbar customization in SfPdfViewer component">documentation section</a>.</p>
    @{
    #endif
