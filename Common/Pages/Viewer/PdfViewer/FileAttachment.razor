    private List<MobileToolbarItem> MobileToolbarItems = new List<MobileToolbarItem>();
    private List<NavigationToolbarItem> navigationToolbarItems = new List<NavigationToolbarItem>();
    private List<CustomNavigationToolbarItem> customNavigationToolbarItems = new List<CustomNavigationToolbarItem>();
    private List<PdfToolbarItem> customToolbarItems = new List<PdfToolbarItem>();
    // Attachment storage; each UploadedFile represents an attachment.
    private List<UploadedFile> uploadedFiles = new List<UploadedFile>();
    // Context menu items for attachment management actions.
    private List<object> ContextMenuItems = new List<object>();
