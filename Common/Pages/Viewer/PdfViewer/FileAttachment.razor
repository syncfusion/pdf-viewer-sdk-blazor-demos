    // References for upload controls.
    private SfUploader? uploadFiles;
    private SfUploader? uploadPdf;
    private SfToast? SfToast;
    // Grid reference for attachment management.
    private SfGrid<UploadedFile>? Grid;
    // Toolbar and navigation item lists for configuring the PDF viewer.
